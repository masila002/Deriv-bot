<xml xmlns="https://developers.google.com/blockly/xml" is_dbot="true" collection="false">
  <variables>
    <variable id="ticks_to_check">ticks_to_check</variable>
    <variable id="digit_found">digit_found</variable>
    <variable id="i">i</variable>
    <variable id="current_digit">current_digit</variable>
    <variable id="tick_list">tick_list</variable>
  </variables>
  <block type="trade_definition" id="trade_def_1" deletable="false" x="0" y="0">
    <statement name="TRADE_OPTIONS">
      <block type="trade_definition_market" id="market_1" deletable="false" movable="false">
        <field name="MARKET_LIST">synthetic_index</field>
        <field name="SUBMARKET_LIST">random_index</field>
        <field name="SYMBOL_LIST">1HZ10V</field>
        <next>
          <block type="trade_definition_tradetype" id="tradetype_1" deletable="false" movable="false">
            <field name="TRADETYPECAT_LIST">digits</field>
            <field name="TRADETYPE_LIST">digits</field>
            <next>
              <block type="trade_definition_contracttype" id="contract_1" deletable="false" movable="false">
                <field name="TYPE_LIST">digitover</field>
                <next>
                  <block type="trade_definition_candleinterval" id="candle_1" deletable="false" movable="false">
                    <field name="CANDLEINTERVAL_LIST">60</field>
                    <next>
                      <block type="trade_definition_restartbuysell" id="restart_1" deletable="false" movable="false">
                        <field name="TIME_MACHINE_ENABLED">FALSE</field>
                        <next>
                          <block type="trade_definition_restartonerror" id="error_1" deletable="false" movable="false">
                            <field name="RESTARTONERROR">TRUE</field>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
    <statement name="SUBMARKET">
      <block type="trade_definition_tradeoptions" id="trade_options_1">
        <mutation xmlns="http://www.w3.org/1999/xhtml" has_first_barrier="false" has_second_barrier="false" has_prediction="true"></mutation>
        <field name="DURATIONTYPE_LIST">t</field>
        <value name="DURATION">
          <shadow type="math_number_positive" id="duration_num">
            <field name="NUM">1</field>
          </shadow>
        </value>
        <value name="AMOUNT">
          <shadow type="math_number_positive" id="amount_num">
            <field name="NUM">0.35</field>
          </shadow>
        </value>
        <value name="PREDICTION">
          <shadow type="math_number" id="prediction_num">
            <field name="NUM">4</field>
          </shadow>
        </value>
      </block>
    </statement>
  </block>
  <block type="before_purchase" id="before_purchase_1" deletable="false" x="0" y="400">
    <statement name="BEFOREPURCHASE_STACK">
      <block type="variables_set" id="set_ticks_1">
        <field name="VAR" id="ticks_to_check">ticks_to_check</field>
        <value name="VALUE">
          <block type="math_number" id="num_10">
            <field name="NUM">10</field>
          </block>
        </value>
        <next>
          <block type="variables_set" id="set_tick_list">
            <field name="VAR" id="tick_list">tick_list</field>
            <value name="VALUE">
              <block type="tick_analysis" id="get_ticks">
                <field name="TICKANALYSIS_LIST">ticks</field>
              </block>
            </value>
            <next>
              <block type="controls_if" id="if_check_length">
                <value name="IF0">
                  <block type="logic_compare" id="compare_length">
                    <field name="OP">EQ</field>
                    <value name="A">
                      <block type="lists_length" id="length_of_list">
                        <value name="VALUE">
                          <block type="variables_get" id="get_tick_list">
                            <field name="VAR" id="tick_list">tick_list</field>
                          </block>
                        </value>
                      </block>
                    </value>
                    <value name="B">
                      <block type="variables_get" id="get_ticks_check">
                        <field name="VAR" id="ticks_to_check">ticks_to_check</field>
                      </block>
                    </value>
                  </block>
                </value>
                <statement name="DO0">
                  <block type="variables_set" id="set_found_false">
                    <field name="VAR" id="digit_found">digit_found</field>
                    <value name="VALUE">
                      <block type="logic_boolean" id="bool_false">
                        <field name="BOOL">FALSE</field>
                      </block>
                    </value>
                    <next>
                      <block type="controls_for" id="for_loop_1">
                        <field name="VAR" id="i">i</field>
                        <value name="FROM">
                          <block type="math_number" id="from_num">
                            <field name="NUM">1</field>
                          </block>
                        </value>
                        <value name="TO">
                          <block type="variables_get" id="get_ticks_to">
                            <field name="VAR" id="ticks_to_check">ticks_to_check</field>
                          </block>
                        </value>
                        <value name="BY">
                          <block type="math_number" id="by_num">
                            <field name="NUM">1</field>
                          </block>
                        </value>
                        <statement name="DO">
                          <block type="variables_set" id="set_current_digit">
                            <field name="VAR" id="current_digit">current_digit</field>
                            <value name="VALUE">
                              <block type="math_modulo" id="modulo_1">
                                <value name="DIVIDEND">
                                  <block type="lists_getIndex" id="get_index">
                                    <mutation statement="false" at="true"></mutation>
                                    <field name="MODE">GET</field>
                                    <field name="WHERE">FROM_START</field>
                                    <value name="VALUE">
                                      <block type="variables_get" id="get_list_var">
                                        <field name="VAR" id="tick_list">tick_list</field>
                                      </block>
                                    </value>
                                    <value name="AT">
                                      <block type="variables_get" id="get_i">
                                        <field name="VAR" id="i">i</field>
                                      </block>
                                    </value>
                                  </block>
                                </value>
                                <value name="DIVISOR">
                                  <block type="math_number" id="div_10">
                                    <field name="NUM">10</field>
                                  </block>
                                </value>
                              </block>
                            </value>
                            <next>
                              <block type="controls_if" id="if_digit_check">
                                <value name="IF0">
                                  <block type="logic_operation" id="or_op">
                                    <field name="OP">OR</field>
                                    <value name="A">
                                      <block type="logic_compare" id="eq_4">
                                        <field name="OP">EQ</field>
                                        <value name="A">
                                          <block type="variables_get" id="get_digit_a">
                                            <field name="VAR" id="current_digit">current_digit</field>
                                          </block>
                                        </value>
                                        <value name="B">
                                          <block type="math_number" id="num_4">
                                            <field name="NUM">4</field>
                                          </block>
                                        </value>
                                      </block>
                                    </value>
                                    <value name="B">
                                      <block type="logic_compare" id="eq_5">
                                        <field name="OP">EQ</field>
                                        <value name="A">
                                          <block type="variables_get" id="get_digit_b">
                                            <field name="VAR" id="current_digit">current_digit</field>
                                          </block>
                                        </value>
                                        <value name="B">
                                          <block type="math_number" id="num_5">
                                            <field name="NUM">5</field>
                                          </block>
                                        </value>
                                      </block>
                                    </value>
                                  </block>
                                </value>
                                <statement name="DO0">
                                  <block type="variables_set" id="set_found_true">
                                    <field name="VAR" id="digit_found">digit_found</field>
                                    <value name="VALUE">
                                      <block type="logic_boolean" id="bool_true">
                                        <field name="BOOL">TRUE</field>
                                      </block>
                                    </value>
                                  </block>
                                </statement>
                              </block>
                            </next>
                          </block>
                        </statement>
                        <next>
                          <block type="controls_if" id="if_final">
                            <value name="IF0">
                              <block type="logic_negate" id="not_found">
                                <value name="BOOL">
                                  <block type="variables_get" id="get_found">
                                    <field name="VAR" id="digit_found">digit_found</field>
                                  </block>
                                </value>
                              </block>
                            </value>
                            <statement name="DO0">
                              <block type="purchase" id="purchase_1">
                                <field name="PURCHASE_LIST">DIGITOVER</field>
                              </block>
                            </statement>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </statement>
              </block>
            </next>
          </block>
        </next>
      </block>
    </statement>
  </block>
  <block type="after_purchase" id="after_purchase_1" x="0" y="900">
    <statement name="AFTERPURCHASE_STACK">
      <block type="trade_again" id="trade_again_1"></block>
    </statement>
  </block>
</xml>
